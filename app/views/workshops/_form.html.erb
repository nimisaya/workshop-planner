<%= form_with model: @workshop, local: true do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <br>
    <%= f.label :private %>
    <%= f.text_field :private %>
    <br>
    <%= f.submit, :class => 'primary-button'%>
<% end %>
<%= form_with model: @workshop, local: true do |f| %>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
    <br>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
    <br>
    <%= f.label :private %><br>
    <%= f.text_field :private %>
    <br>
    <%= f.submit :class => 'primary-button button-link'%>
<% end %>